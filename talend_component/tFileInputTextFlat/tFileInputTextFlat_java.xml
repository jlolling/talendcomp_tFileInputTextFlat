<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER PLATEFORM="ALL" SERIAL="" VERSION="4.3" STATUS="PROD" COMPATIBILITY="ALL" AUTHOR="Jan Lolling" RELEASE_DATE="20170127" STARTABLE="true" HAS_CONDITIONAL_OUTPUTS="true"> 
    <SIGNATURE/> 
  </HEADER>  
  <FAMILIES> 
    <FAMILY>File/Input</FAMILY> 
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL/> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1"/>  
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1"/>  
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1"/>  
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1"/>  
    <CONNECTOR CTYPE="COMPONENT_OK"/>  
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>  
    <CONNECTOR CTYPE="RUN_IF"/>  
    <CONNECTOR NAME="REJECT" CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" LINE_STYLE="2" COLOR="FF0000"/> 
  </CONNECTORS>  
  <PARAMETERS> 
    <PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="10" REPOSITORY_VALUE="DELIMITED"/>  
    <PARAMETER NAME="FILENAME" FIELD="FILE" NUM_ROW="20" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH"> 
      <DEFAULT>"/path/to/your/file"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="30" REQUIRED="true" REPOSITORY_VALUE="ENCODING"> 
      <DEFAULT>"UTF-8"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="IGNORE_BOM" FIELD="CHECK" NUM_ROW="30" REQUIRED="true"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="LINES_TO_SKIP" FIELD="TEXT" NUM_ROW="40"> 
      <DEFAULT>0</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="REMOVE_EMPTY_ROW" FIELD="CHECK" REQUIRED="true" NUM_ROW="40" REPOSITORY_VALUE="REMOVE_EMPTY_ROW"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="50"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER NAME="COLUMN_HEADER_LINE" FIELD="CHECK" NUM_ROW="60"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="CONFIG_DELIMITED_BY_HEADER" FIELD="CHECK" REQUIRED="true" NUM_ROW="60" SHOW_IF="COLUMN_HEADER_LINE == 'true'"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="CONFIG_HEADER_BY_REGEX" FIELD="CHECK" REQUIRED="true" NUM_ROW="60" SHOW_IF="(COLUMN_HEADER_LINE == 'true') and (CONFIG_DELIMITED_BY_HEADER == 'true')"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="IGNORE_NOT_NULL" FIELD="CHECK" REQUIRED="true" NUM_ROW="65"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="LOAD_CONFIG_FROM_FILE" FIELD="CHECK" NUM_ROW="67"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="CONFIG_FILE" FIELD="FILE" REQUIRED="true" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'true'" NUM_ROW="67"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER NAME="FIELD_CONFIG" FIELD="TABLE" REQUIRED="true" NUM_ROW="80" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'" NB_LINES="10"> 
      <ITEMS BASED_ON_SCHEMA="true"> 
        <ITEM NAME="POSITIONING_TYPE" FIELD="CLOSED_LIST"> 
          <ITEMS DEFAULT="DELIMITED"> 
            <ITEM NAME="DELIMITED" VALUE="DEL"/>  
            <ITEM NAME="ABSOLUT_POS" VALUE="ABS"/> 
          </ITEMS> 
        </ITEM>  
        <ITEM NAME="POSITION" FIELD="TEXT"/>  
        <ITEM NAME="LENGTH" FIELD="TEXT"/>  
        <ITEM NAME="REGEX" FIELD="TEXT"/>  
        <ITEM NAME="ALTERNATIVE" FIELD="TEXT" SHOW_IF="ALLOW_ALT_COLUMNS == 'true'"/>  
        <ITEM NAME="COLUMN_HEADER_NAME" FIELD="TEXT" SHOW_IF="(COLUMN_HEADER_LINE == 'true') and (CONFIG_DELIMITED_BY_HEADER == 'true')"/>  
        <ITEM NAME="IGNORE_IF_MISSING" FIELD="CHECK" SHOW_IF="(COLUMN_HEADER_LINE == 'true') and (CONFIG_DELIMITED_BY_HEADER == 'true')"/> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER NAME="ALLOW_ALT_COLUMNS" FIELD="CHECK" REQUIRED="true" NUM_ROW="85" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="LIMIT_STRING_CONTENT_TO_LENGTH" FIELD="CHECK" REQUIRED="true" NUM_ROW="85" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="FIELDSEPARATOR" FIELD="TEXT" NUM_ROW="90" REQUIRED="true" REPOSITORY_VALUE="FIELD_SEPARATOR" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>"|"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="USE_ENCLOSURES" FIELD="CHECK" NUM_ROW="110" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="TEXT_ENCLOSURE" FIELD="TEXT" NUM_ROW="110" REPOSITORY_VALUE="TEXT_ENCLOSURE" SHOW_IF="(LOAD_CONFIG_FROM_FILE == 'false') and (USE_ENCLOSURES == 'true')"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER NAME="PARSE_ROW_FIRST" FIELD="CHECK" NUM_ROW="110" SHOW_IF="(LOAD_CONFIG_FROM_FILE == 'false') and (USE_ENCLOSURES == 'true')"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="ALLOW_ENCLUSURE_IN_TEXT" FIELD="CHECK" NUM_ROW="110" SHOW_IF="(LOAD_CONFIG_FROM_FILE == 'false') and (USE_ENCLOSURES == 'true')"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="NUMBER_COUNTRY" FIELD="TEXT" REQUIRED="true" NUM_ROW="120" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>"en"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="DEFAULT_DATE_FORMAT" FIELD="TEXT" REQUIRED="true" NUM_ROW="120" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'"> 
      <DEFAULT>"dd-MM-yyyy"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="130"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="SCHEMA_REJECT" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="140" CONTEXT="REJECT" SHOW_IF="DIE_ON_ERROR == 'false'"> 
      <TABLE READONLY="true"> 
        <COLUMN NAME="lineNumber" TYPE="id_Integer" LENGTH="10" READONLY="true" CUSTOM="true"/>  
        <COLUMN NAME="line" TYPE="id_String" LENGTH="4000" READONLY="true" CUSTOM="true"/>  
        <COLUMN NAME="errorMessage" TYPE="id_String" LENGTH="255" READONLY="true" CUSTOM="true"/> 
      </TABLE> 
    </PARAMETER> 
  </PARAMETERS>  
  <ADVANCED_PARAMETERS> 
    <PARAMETER NAME="DEBUG" FIELD="CHECK" NUM_ROW="10"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="SAVE_CONFIG_TO_FILE" FIELD="CHECK" SHOW_IF="LOAD_CONFIG_FROM_FILE == 'false'" NUM_ROW="20"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="CONFIG_FILE_BACKUP" FIELD="FILE" REQUIRED="true" SHOW_IF="(SAVE_CONFIG_TO_FILE == 'true') and (LOAD_CONFIG_FROM_FILE == 'false')" NUM_ROW="20"> 
      <DEFAULT/> 
    </PARAMETER>  
    <PARAMETER NAME="ALLOW_FIELDS_NOT_FOUND_IN_CURRENT_FILE" FIELD="CHECK" NUM_ROW="25" SHOW_IF="(SAVE_CONFIG_TO_FILE == 'true') and (LOAD_CONFIG_FROM_FILE == 'false')"> 
      <DEFAULT>true</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="TRIMALL" FIELD="CHECK" NUM_ROW="30"> 
      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="TRIMSELECT" FIELD="TABLE" NUM_ROW="40" NB_LINES="10" SHOW_IF="TRIMALL=='false'"> 
      <ITEMS BASED_ON_SCHEMA="true"> 
        <ITEM NAME="TRIM" FIELD="CHECK"/> 
      </ITEMS> 
    </PARAMETER>  
    <PARAMETER NAME="RELEASE_LABEL_20170127" FIELD="LABEL" COLOR="0;0;0" NUM_ROW="900">
      <DEFAULT>Release: 4.3 build at: 20170127</DEFAULT>
    </PARAMETER>
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION> 
    <IMPORTS> 
      <IMPORT NAME="cimt-talendcomp-flatfileimport" MODULE="cimt-talendcomp-flatfileimport-4.3.jar" REQUIRED="true"/>
    </IMPORTS> 
  </CODEGENERATION>  
  <RETURNS> 
    <RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER"/>  
    <RETURN NAME="NB_REJECTED" TYPE="id_Integer" AVAILABILITY="AFTER"/>  
    <RETURN NAME="FILENAME" TYPE="id_String" AVAILABILITY="AFTER"/>  
    <RETURN NAME="HEADER_CONFIG_ERROR" TYPE="id_Boolean" AVAILABILITY="AFTER"/> 
  </RETURNS>
</COMPONENT>
